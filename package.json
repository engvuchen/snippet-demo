{
  "name": "snippet-demo",
  "displayName": "snippet-demo",
  "description": "简单易上手的 snippet vscode 插件模板",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "pkg": "node ./tools/pkg/index.js",
    "update-snippet": "node ./tools/update-snippet/index.js",
    "docs": "node tools/docs/index.js"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "proto3",
        "path": "./snippets/proto3.json"
      }
    ]
  },
  "snippetFilter": [
    {
      "language": "javascript",
      "includes": [
        "@util"
      ]
    },
    {
      "language": "vue",
      "includes": []
    },
    {
      "language": "proto3",
      "includes": []
    }
  ]
}